{
  "id": "/uengine-billing",
  "backoffFactor": 1.15,
  "backoffSeconds": 1,
  "constraints": [
    [
      "hostname",
      "LIKE",
      "172.31.24.25"
    ]
  ],
  "container": {
    "portMappings": [
      {
        "containerPort": 8080,
        "hostPort": 0,
        "protocol": "tcp",
        "servicePort": 13600
      }
    ],
    "type": "DOCKER",
    "volumes": [
      {
        "containerPath": "/pinpoint-agent",
        "hostPath": "/pinpoint-agent",
        "mode": "RW"
      }
    ],
    "docker": {
      "image": "sppark/billing:v2-1",
      "forcePullImage": false,
      "privileged": false,
      "parameters": [
        {
          "key": "label",
          "value": "APP_NAME=uengine-billing"
        }
      ]
    }
  },
  "cpus": 1.8,
  "disk": 0,
  "env": {
    "IAM_CLIENT_SECRET": "my-client-secret",
    "IAM_CLIENT_KEY": "my-client-key",
    "EVENT_SERVER_HOST": "http://marathon-lb-internal.marathon.mesos:21999/event",
    "DB_DATABASE": "killbill",
    "IAM_HOST": "marathon-lb-internal.marathon.mesos",
    "DB_HOST": "cloud.pas-mini.io",
    "IAM_PORT": "10133",
    "DB_USER": "root",
    "DB_PORT": "13306",
    "DB_PASSWORD": "killbill",
    "ADMIN_USERNAME": "billing@uengine.org",
    "ADMIN_PASSWORD": "gosu23546",
    "TEST_MODE": "true"
  },
  "healthChecks": [
    {
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "maxConsecutiveFailures": 3,
      "timeoutSeconds": 20,
      "delaySeconds": 15,
      "protocol": "COMMAND",
      "command": {
        "value": "pwd"
      }
    }
  ],
  "instances": 1,
  "labels": {
    "HAPROXY_GROUP": "external,internal",
    "HAPROXY_0_REDIRECT_TO_HTTPS": "false",
    "HAPROXY_0_VHOST": "billing.pas-mini.io"
  },
  "maxLaunchDelaySeconds": 3600,
  "mem": 2700,
  "gpus": 0,
  "networks": [
    {
      "mode": "container/bridge"
    }
  ],
  "requirePorts": false,
  "upgradeStrategy": {
    "maximumOverCapacity": 1,
    "minimumHealthCapacity": 0
  },
  "killSelection": "YOUNGEST_FIRST",
  "unreachableStrategy": {
    "inactiveAfterSeconds": 0,
    "expungeAfterSeconds": 0
  },
  "fetch": []
}